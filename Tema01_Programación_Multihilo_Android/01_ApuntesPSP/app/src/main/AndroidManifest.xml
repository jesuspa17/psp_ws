<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dam.salesianostriana.psp.ejemploasynctask" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AsynctaskActivity" >
        </activity>
        <activity android:name=".ThreadActivity" >
        </activity>
        <activity android:name=".ExecutorActivity" >
        </activity>
        <activity android:name=".BufferedReaderActivity" >
        </activity>
        <activity android:name=".BufferedWriterActivity" >
        </activity>
        <activity android:name=".ByteArrayOutputStreamActivity" >
        </activity>
        <activity android:name=".ByteArrayInputStreamActivity" >
        </activity>
    </application>

</manifest>
